{
    "manifest": {
        "translationVersion": 1733912516869,
        "pluginVersion": "1.1.2"
    },
    "description": {
        "original": "Add the current note's reading time to Obsidian's status bar.",
        "translation": "Add the current note's reading time to Obsidian's status bar."
    },
    "dict": {
        "name: \"Selected Text\"": "name: \"所选文本\"",
        ".setText(\"0 min read\")": ".setText(\"0 分阅读\")",
        ".setName(\"Reading speed\")": ".setName(\"阅读速度\")",
        ".setName(\"Format\")": ".setName(\"格式\")",
        ".setName(\"Append Text\")": ".setName(\"附加文本\")",
        ".setDesc(\"Words per minute used for reading speed (default: 200).\")": ".setDesc(\"用于阅读速度的每分钟字数(默认: 200).\")",
        ".setDesc(\"Choose the output format\")": ".setDesc(\"选择输出格式\")",
        ".setDesc(\"Append 'read' to formatted string.\")": ".setDesc(\"将 '字符串' 附加到格式化字符串。\")",
        ".setPlaceholder(\"Example: 200\")": ".setPlaceholder(\"例如: 200\")",
        "addOption(\"compact\", \"Compact (10m)\")": "addOption(\"compact\", \"紧凑(10分)\")",
        "addOption(\"simple\", \"Simple (10m 4s)\")": "addOption(\"simple\", \"简单 (10分4秒)\")",
        "addOption(\"verbose\", \"Verbose (10 minutes 4 seconds)\")": "addOption(\"verbose\", \"详细(10分钟4秒)\")",
        "addOption(\"digital\", \"Colon Notation (10:04)\")": "addOption(\"digital\", \"冒号表示法 (10:04)\")",
        "return plugin.settings.appendText ? `${result.minutes} min read` : `${result.minutes} min`;": "return plugin.settings.appendText ? `${result.minutes} 分` : `${result.minutes} 分`;",
        "add(parsed.days % 365, \"day\", \"d\");": "add(parsed.days % 365, \"天\", \"天\");",
        "add(parsed.hours, \"hour\", \"h\");": "add(parsed.hours, \"小时\", \"时\");",
        "add(parsed.minutes, \"minute\", \"m\");": "add(parsed.minutes, \"分钟\", \"分\");",
        "add(parsed.seconds, \"second\", \"s\");": "add(parsed.seconds, \"秒\", \"秒\");",
        "add(parsed.milliseconds, \"millisecond\", \"ms\");": "add(parsed.milliseconds, \"毫秒\", \"毫秒\");",
        "add(parsed.microseconds, \"microsecond\", \"\\xB5s\");": "add(parsed.microseconds, \"微秒\", \"\\微秒\");",
        "add(parsed.nanoseconds, \"nanosecond\", \"ns\");": "add(parsed.nanoseconds, \"纳秒\", \"纳秒\");",
        "add(Number.parseFloat(secondsString, 10), \"second\", \"s\", secondsString);": "add(Number.parseFloat(secondsString, 10), \"秒\", \"秒\", secondsString);",
        "var pluralize = (word, count) => (count === 1 ? word : `${word}s`);": "var pluralize = (word, count) => (count === 1 ? word : `${word}`);",
        ".addOption(\"default\", \"Default (10 min)\")": ".addOption(\"default\", \"默认(10 分)\")"
    }
}
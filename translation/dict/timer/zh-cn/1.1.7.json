    "dict": {
        "Notice(\"Invalid value\")": "Notice(\"无效值\")",
        "Notice(\n              \"This browser does not support desktop notifications.\"\n            )": "Notice(\n              \"此浏览器不支持桌面通知。\"\n            )",
        "Notice(\n                  \"You need to grant permission to receive notifications.\"\n                )": "Notice(\n                  \"您需要授权接收通知。\"\n                )",
        "Notice(\"Timer is finished!!\")": "Notice(\"计时器完成!!\")",
        "name: \"Open timer\"": "name: \"开启计时器\"",
        "name: \"Set timer to\"": "name: \"设置计时器为\"",
        "name: \"Add favorite timer\"": "name: \"添加收藏计时器\"",
        "name: \"Use one of favorite timers\"": "name: \"使用其中一个收藏计时器\"",
        "name: \"Remove one of favorite timers\"": "name: \"移除一个收藏计时器\"",
        "text: \"1. In time notation: HH:MM:SS.\"": "text: \"1. 时间表示法：HH:MM:SS.\"",
        "text: \"2. With letters: 00h00m00s.\"": "text: \"2. 使用字母表示：00h00m00s.\"",
        "text: \"Timer\"": "text: \"计时器\"",
        ".setButtonText(\"Submit\")": ".setButtonText(\"提交\")",
        ".setName(`${i + 1} timer button value`)": ".setName(`${i + 1} 计时器按钮值`)",
        ".setName(\"Stack timer buttons\")": ".setName(\"堆叠计时器按钮\")",
        ".setName(\"Use verbose time format\")": ".setName(\"使用详细时间格式\")",
        ".setName(\"Remove not set values in verbose time format\")": ".setName(\"在详细时间格式中移除未设置的值\")",
        ".setName(\"Use OS notification\")": ".setName(\"使用操作系统通知\")",
        ".setDesc(`Set the value which increases / decreases the timer if clicking on the ${i + 1} timer button. \n                      The value contains a number followed by seconds(type s) or minutes(type m) or hours(type h) & number \n                      must be greater than 0.`)": ".setDesc(`设置增加或减少计时器的值，如果点击第${i + 1}个计时器按钮。\n                      该值包含一个数字，后面跟着秒（类型s）、分钟（类型m）或小时（类型h）和数字\n                      必须大于0.`)",
        ".setDesc(\n      \"If enabled, the timer increment buttons will be stacked on top of the timer decrement buttons. If disabled, the timer increment buttons will be placed to the right of the timer decrement buttons.\"\n    )": ".setDesc(\n      \"如果启用，计时器增加按钮将堆叠在计时器减少按钮之上。如果禁用，计时器增加按钮将放置在计时器减少按钮的右侧。\"\n    )",
        ".setDesc(\n      'If enabled, the timer display will be in the format of \"1h 2m 3s\" instead of \"01:02:03\".'\n    )": ".setDesc(\n      '如果启用，计时器显示将以\"1h 2m 3s\"的格式，而不是\"01:02:03\".'\n    )",
        ".setDesc(\n      \"If enabled and verbose enabled, 00 values are not shown. If disabled and for example +1m is clicked, also '00s' is shown\"\n    )": ".setDesc(\n      \"如果启用并且启用了详细模式，将不显示00值。如果禁用，例如点击了+1m，也会显示'00s'\"\n    )",
        ".setDesc(\n      \"If enabled, the timer notification will be an OS-level notification rather than an Obisidian notice, and will remain active until dismissed.\"\n    )": ".setDesc(\n      \"如果启用，计时器通知将是一个操作系统级别的通知，而不是一个Obsidian通知，并且会一直活跃直到被关闭。\"\n    )",
        ".setPlaceholder(\"Enter value\")": ".setPlaceholder(\"输入值\")",
        "children: \"Hours\"": "children: \"时\"",
        "children: \"Minutes\"": "children: \"分\"",
        "children: \"Seconds\"": "children: \"秒\"",
        "children: \"Start\"": "children: \"开始\"",
        "children: \"Reset\"": "children: \"重置\"",
        "children: \"Cancel\"": "children: \"取消\"",
        "\"Pause\"": "\"暂停\"",
        "\"Resume\"": "\"继续\""

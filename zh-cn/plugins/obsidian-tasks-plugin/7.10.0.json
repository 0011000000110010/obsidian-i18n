        ".createEl(\"h4\",{text:\"Task Statuses\"})": ".createEl(\"h4\",{text:\"任务状态\"})",
        ".createEl(\"h4\",{text:\"Date Settings\"})": ".createEl(\"h4\",{text:\"日期设置\"})",
        ".createEl(\"h4\",{text:\"Recurring task Settings\"})": ".createEl(\"h4\",{text:\"常规任务设置\"})",
        ".createEl(\"h4\",{text:\"Auto-suggest Settings\"})": ".createEl(\"h4\",{text:\"自动建议设置\"})",
        ".createEl(\"h4\",{text:\"Dialog Settings\"})": ".createEl(\"h4\",{text:\"对话设置\"})"

{
    "manifest": {
        "id": "obsidian-style-settings",
        "author": "曲淡歌",
        "version": "1.0.1",
        "pluginVersion": "1.0.9"
    },
    "description": {
        "original": "Offers controls for adjusting theme, plugin, and snippet CSS variables.",
        "translation": "提供调整主题、插件和代码片段 CSS 变量的控制选项。"
    },
    "dict": {
        ".setText(`Error importing style settings: ${p}`)": ".setText(`导入样式设置时出错：${p}`)",
        ".setText(\"Error importing style settings: config is empty\")": ".setText(\"导入样式设置时出错：配置为空\")",
        ".setText(`${this.filterResultCount} Results`)": ".setText(`${this.filterResultCount} 结果`)",
        ".setButtonText(\"Save\")": ".setButtonText(\"保存\")",
        ".setName(`Export settings for: ${this.section}`)": ".setName(`导出设置：${this.section}`)",
        ".setName(\"Import style setting\")": ".setName(\"导入样式设置\")",
        ".setDesc(\"Import an entire or partial configuration. Warning: this may override existing settings\")": ".setDesc(\"导入整个或部分配置。警告：这可能会覆盖现有设置。\")",
        ".setPlaceholder(\"Paste config here...\")": ".setPlaceholder(\"在此处粘贴配置…\")",
        ".setPlaceholder(\"Search Style Settings...\")": ".setPlaceholder(\"搜索样式设置...\")",
        ".setTooltip(\"Reset all settings to default\")": ".setTooltip(\"重置所有设置为默认值\")",
        ".setTooltip(\"Export settings\")": ".setTooltip(\"导出设置\")",
        ".appendText(\"Style settings configured by theme and plugin authors will show up here. You can also create your own configuration by creating a CSS snippet in your vault. \")": ".appendText(\"风格设置将在此处显示，这些设置由主题和插件作者配置。您也可以在保险库中创建一个 CSS 代码片段来创建自己的配置。\")",
        ".createEl(\"a\",{text:\"Click here for details and examples.\",href:\"https://github.com/mgmeyers/obsidian-style-settings#obsidian-style-settings-plugin\"})": ".createEl(\"a\",{text:\"点击此处获得细节与示例。\",href:\"https://github.com/mgmeyers/obsidian-style-settings#obsidian-style-settings-plugin\"})"
    }
}
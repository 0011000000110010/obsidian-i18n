{
    "manifest": {
        "id": "obsidian-folder-index",
        "author": "里安",
        "version": "1.0.0",
        "pluginVersion": "1.0.26"
    },
    "description": {
        "original": "This Plugin will automatically generate a TOC for the current Folder.",
        "translation": "此插件将自动为当前文件夹生成目录。"
    },
    "dict": {
        ".setButtonText(\"Reset\")": ".setButtonText(\"重置\")",
        ".setName(\"Overwrite Graph View\")": ".setName(\"覆盖图形视图\")",
        ".setName(\"Root Index File\")": ".setName(\"根索引文件\")",
        ".setName(\"Initial Content\")": ".setName(\"初始内容\")",
        ".setName(\"Excluded Folders\")": ".setName(\"排除文件夹\")",
        ".setName(\"Automatically generate IndexFile\")": ".setName(\"自动生成索引文件\")",
        ".setName(\"Automatically Rename IndexFile\")": ".setName(\"自动重命名索引文件\")",
        ".setName(\"User defined index filename\")": ".setName(\"用户定义的索引文件名\")",
        ".setName(\"Index filename\")": ".setName(\"索引文件名\")",
        ".setName(\"Hide IndexFile\")": ".setName(\"隐藏索引文件\")",
        ".setName(\"Auto include preview\")": ".setName(\"自动包含预览\")",
        ".setName(\"Disable Headlines\")": ".setName(\"禁用标题\")",
        ".setName(\"Headline Limit\")": ".setName(\"标题限制\")",
        ".setName(\"Automatic Preview mode\")": ".setName(\"自动预览模式\")",
        ".setName(\"Sort Files Alphabetically\")": ".setName(\"按字母顺序排序文件\")",
        ".setName(\"Sort Headers Alphabetically\")": ".setName(\"按字母顺序排序标题\")",
        ".setName(\"Build IndexFiles Recursively\")": ".setName(\"递归构建索引文件\")",
        ".setName(\"Subfolder Limit\")": ".setName(\"子文件夹限制\")",
        ".setName(\"Render Folders Bold\")": ".setName(\"粗体显示文件夹\")",
        ".setName(\"Render Folder Italic\")": ".setName(\"斜体显示文件夹\")",
        ".setName(\"Use bullet-points instead of list\")": ".setName(\"使用项目符号代替列表\")",
        ".setDesc(\"This will overwrite the default graph view and link files based on their index as well as their normal links\")": ".setDesc(\"这将覆盖默认的图形视图，并根据其索引以及常规链接来链接文件。\")",
        ".setDesc(\"The File that is used for the Root Index File\")": ".setDesc(\"用于根索引文件的文件。\")",
        ".setDesc(\"Set the initial content for new folder indexes.\")": ".setDesc(\"设置新文件夹索引的初始内容。\")",
        ".setDesc(\"These Folders will not automatically create an IndexFile\")": ".setDesc(\"这些文件夹将不会自动创建索引文件。\")",
        ".setDesc(\"This will automatically create an IndexFile when you create a new folder\")": ".setDesc(\"当你创建一个新的文件夹时，这将自动创建一个索引文件。\")",
        ".setDesc(\"This will automatically rename the folders index file as you rename folders\")": ".setDesc(\"当你重命名文件夹时，这将自动重命名文件夹的索引文件。\")",
        ".setDesc(\"This will automatically create an IndexFile with the user defined name\")": ".setDesc(\"这将使用用户定义的名称自动创建索引文件。\")",
        ".setDesc(\"the filename that is used as the folder index\")": ".setDesc(\"用作文件夹索引的文件名。\")",
        ".setDesc(\"This will hide IndexFiles from the file explorer (Disabled as it causes bugs right now)\")": ".setDesc(\"这将隐藏文件浏览器中的索引文件（由于目前存在bug已禁用）。\")",
        ".setDesc(\"This will automatically include previews when creating index files (!) \")": ".setDesc(\"这将在创建索引文件时自动包含预览（！）。\")",
        ".setDesc(\"This will disable listing headlines within the index file\")": ".setDesc(\"这将禁用在索引文件中列出标题。\")",
        ".setDesc(\"Limit the Depth of Headlines Displayed\")": ".setDesc(\"限制显示的标题深度。\")",
        ".setDesc(\"This will automatically swap to preview mode when opening an index file\")": ".setDesc(\"这将在打开索引文件时自动切换到预览模式。\")",
        ".setDesc(\"This will sort the Files alphabetically\")": ".setDesc(\"这将按字母顺序排序文件。\")",
        ".setDesc(\"This will sort the Headers within a file alphabetically\")": ".setDesc(\"这将按字母顺序排序文件内的标题。\")",
        ".setDesc(\"This will show all files within a folder and its subfolders\")": ".setDesc(\"这将显示文件夹及其子文件夹中的所有文件。\")",
        ".setDesc(\"Limit the Depth of Subfolders(-1 for no limit)\")": ".setDesc(\"限制子文件夹的深度（-1表示不限制）。\")",
        ".setDesc(\"This will render folders in **bold**\")": ".setDesc(\"这将以**粗体**显示文件夹。\")",
        ".setDesc(\"This will render folders in *italic*\")": ".setDesc(\"这将以*斜体*显示文件夹。\")",
        ".setDesc(\"This will render the index file as a bullet-point list instead of a numbered list\")": ".setDesc(\"这将索引文件渲染为项目符号列表而不是编号列表。\")",
        ".setPlaceholder(\"dashboard.md\")": ".setPlaceholder(\"dashboard.md\")",
        ".setPlaceholder(\"About the folder.\")": ".setPlaceholder(\"关于文件夹。\")",
        ".setPlaceholder(\"Folder1\\nFolder2/Foo\\nFolder3/Foo/Bar\")": ".setPlaceholder(\"Folder1\\nFolder2/Foo\\nFolder3/Foo/Bar\")",
        ".setPlaceholder(\"!.md\")": ".setPlaceholder(\"!.md\")",
        ".setPlaceholder(\"6\")": ".setPlaceholder(\"6\")",
        ".setPlaceholder(\"-1\")": ".setPlaceholder(\"-1\")",
        ".setTooltip(\"Reset to default\")": ".setTooltip(\"恢复默认值\")",
        ".createEl(\"h2\", { text: \"Graph Settings\" })": ".createEl(\"h2\", { text: \"图形设置\" })",
        ".createEl(\"h2\", { text: \"Index File Settings\" })": ".createEl(\"h2\", { text: \"索引文件设置\" })",
        ".createEl(\"h2\", { text: \"Content Renderer Settings\" })": ".createEl(\"h2\", { text: \"内容渲染器设置\" })"
    }
}
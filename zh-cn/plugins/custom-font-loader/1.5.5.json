{
  "manifest": {
    "id": "custom-font-loader",
    "author": "张海山",
    "version": "1.0.1",
    "pluginVersion": "1.5.5"
  },
  "description": {
    "original": "Customize your Obsidian vault with any font you want (+ Support for Android and IOS)",
    "translation": "使用任何您想要的字体自定义您的 Obsidian 保险库（+ 支持 Android 和 iOS）"
  },
  "dict": {
    "Notice(error)": "Notice(error)",
    "Notice(\"Processing Font files\")": "Notice(\"处理字体文件\")",
    ".log(\"loading %s\", font_file_name)": ".log(\"正在加载%s\", font_file_name)",
    ".log(\"saved font %s into %s\", font_family_name, css_font_path)": ".log(\"已将字体%s保存到%s\", font_family_name, css_font_path)",
    ".log(\"Font CSS Saved into %s\", css_font_path)": ".log(\"字体CSS已保存到%s\", css_font_path)",
    ".log(error)": ".log(error)",
    "name: \"none\"": "name: \"没有\"",
    "name: \"all\"": "name: \"全部的\"",
    ".setText(\n      \"In Order to set the font, copy your font into fonts directory that you set\"\n    )": ".setText(\n      \"要设置字体，请将字体复制到您设置的字体目录中\"\n    )",
    ".setButtonText(\"Reload\")": ".setButtonText(\"重新加载\")",
    ".setName(\"Fonts Folder\")": ".setName(\"字体文件夹\")",
    ".setName(\"Reload fonts from folder\")": ".setName(\"从文件夹中重新加载字体\")",
    ".setName(\"Font\")": ".setName(\"字体\")",
    ".setName(\"Force Style\")": ".setName(\"力量风格\")",
    ".setName(\"Custom CSS Mode\")": ".setName(\"自定义CSS模式\")",
    ".setName(\"Custom CSS Style\")": ".setName(\"自定义CSS样式\")",
    ".setDesc(\"Folder to look for your custom fonts\")": ".setDesc(\"查找自定义字体的文件夹\")",
    ".setDesc(\n      \"This button reloades from the folder you specified (it also creates the folder for you)\"\n    )": ".setDesc(\n      \"此按钮将从您指定的文件夹重新加载（它还会为您创建文件夹）\"\n    )",
    ".setDesc(\n      `Choose font (If you can't see your fonts, make sure your fonts are in the folder you specified and hit reload. \n\t\t\t\tAlso if you choose multiple fonts option, we will load and process all fonts in the folder for you. In that Case, enable Custom CSS Mode)`\n    )": ".setDesc(\n      `选择字体（如果你看不到你的字体，请确保你的字体在你指定的文件夹中，然后点击重新加载。另外，如果你选择多个字体选项，我们将为你加载并处理该文件夹中的所有字体。在这种情况下，请启用自定义 CSS 模式）`\n    )",
    ".setDesc(\n      \"This option should only be used if you have installed a community theme and normal mode doesn't work\"\n    )": ".setDesc(\n      \"仅当您安装了社区主题并且正常模式不起作用时，才应使用此选项\"\n    )",
    ".setDesc(\n      \"If you want to apply a custom css style rather than default style, choose this.\"\n    )": ".setDesc(\n      \"如果你想应用自定义css样式而不是默认样式，请选择此选项。\"\n    )",
    ".setDesc(\"Input your custom css style\")": ".setDesc(\"输入自定义css样式\")"
  }
}